## Copy to .env and fill the REQUIRED values
## Minimal required vars (others optional / auto-generated):
##  SCALEKIT_ENVIRONMENT_URL, SCALEKIT_CLIENT_ID, SCALEKIT_CLIENT_SECRET, JWT_SECRET

PORT=3000
NODE_ENV=development

# REQUIRED: Scalekit credentials
SUPABASE_URL=https://your-supabase-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# REQUIRED: JWT secret (for demo token issuance). Use strong value in production.
JWT_SECRET=your_jwt_secret_here_change_me
SCALEKIT_ENVIRONMENT_URL=your-scalekit-env-url
SCALEKIT_CLIENT_ID=your-scalekit-client-id
SCALEKIT_CLIENT_SECRET=your-scalekit-client-secret
